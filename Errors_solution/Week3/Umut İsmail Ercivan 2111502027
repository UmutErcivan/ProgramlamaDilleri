import java.util.Scanner;

public class EvdenCik {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.println("Yaşınızı giriniz:");
        int yas = scanner.nextInt();

        System.out.println("Cinsiyetinizi seçin (E/K):");
        char cinsiyet = scanner.next().charAt(0);

        System.out.println("Adınızın ilk harfini giriniz:");
        String ad = scanner.next();

        if (yas > 16 && yas < 18 && cinsiyet == 'E' && ad.equals("B")) {
            System.out.println("Evde kal");
        } else {
            System.out.println("Evden çık");
        }
    }
}
